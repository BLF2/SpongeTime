<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.updev.Events.Group.EventsGroup" table="group_events">
        <cache usage="read-write"/>
        <id name="egId" column="eg_id">
            <generator class="native"/>
        </id>
        <property name="eventId" type="java.lang.Integer" column="event_group_id"/>
        <property name="groupId" type="java.lang.Integer" column="event_group_id"/>
        <property name="userId" type="java.lang.Integer" column="event_group_id"/>
        <property name="isFinish" type="java.lang.Boolean" column="event_group_id"/>
        <property name="finishTime" type="java.util.Date" column="event_group_id"/>
    </class>
</hibernate-mapping>