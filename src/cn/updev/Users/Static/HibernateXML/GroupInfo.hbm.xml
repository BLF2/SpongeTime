<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.updev.Users.Group.GroupInfo.GroupInfo" table="user_groups">
        <cache usage="read-write"/>
        <id name="groupId" column="user_group_id">
            <generator class="native"/>
        </id>
        <property name="groupName" type="java.lang.String" column="user_group_name"/>
        <property name="groupIntro" type="java.lang.String" column="user_group_intro"/>
    </class>
</hibernate-mapping>